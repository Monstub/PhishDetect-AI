{
    "manifest_version": 3,
    "name": "Cheat Sheep Extension",
    "description": "Helps you detect phishing websites.",
    "version": "1.0",
    "action": {
      "default_popup": "src/popup.html",
      "default_icon": "src/assets/shield.png"
    },
    "permissions": [
      "tabs",
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "*://*/*"
    ]
  }